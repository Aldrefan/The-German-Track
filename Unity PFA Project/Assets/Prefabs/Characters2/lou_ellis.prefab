MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 6373200947820980919}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: da0600b722111204dba0b7a1e797af80, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  PNJName: Dr. Lou Ellis
  dialogIndex: 0
  transitionQuote: 5
  haveEvent: 0
  stickerRedirection:
    stickerGivenList: 080000000d0000000e000000
    redirectionList: 020000000300000004000000
  eventRedirection:
    eventGivenList: 
    redirectionEventList: 
  allDialogs:
    listOfDialogs:
    - dialog:
      - quote: "Mais que faites-vous ici ? C\u2019est une zone interdite au public."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "J\u2019ai un laisser-passer. Docteur Ellis, je dois vous parler \xE0
          propos de Loraine Grey."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oh excusez-moi, pour la plupart des gens c\u2019est mon p\xE8re, le
          Docteur Ellis."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous pouvez m\u2019appeler Lou."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Comme vous voudrez.
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Je suis curieuse de voir ce que vous allez me demander.
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Finalement vous souhaitez \xE9couter ce que j\u2019ai \xE0 dire."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je ne fais pas partie de la police. Je suis priv\xE9. Je viens de
          la part de Clara Grey."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Ah oui ? Une femme charmante.
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Si on veut\u2026 Il parait que vous avez une th\xE9orie sur la substance
          retrouv\xE9e dans le sang de Loraine."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est plus qu\u2019une th\xE9orie Monsieur\u2026"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Quel est votre nom \xE0 propos ?"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Kenneth.
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\u2026Kenneth. Ce sont des faits av\xE9r\xE9s."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Voyez-vous, La victime a ing\xE9r\xE9 une puissante toxine qui lui
          aurait servie, en th\xE9orie, \xE0 provoquer une overdose."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Ce n\u2019est pas le cas ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Non. La quantit\xE9 de toxique ing\xE9r\xE9e lui aurait, dans le pire
          des cas, fait sauter le petit d\xE9jeuner."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Impossible de faire une overdose avec \xE7a."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Et quand bien m\xEAme la dose aurait \xE9t\xE9 suffisante, pourquoi
          se pendre en plus ?"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\xC7a semble \xEAtre des souffrances superflues."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Peut-\xEAtre voulait-elle simplement se d\xE9tendre avant de grimper
          sur le tabouret."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Impossible.
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Etant donn\xE9e la r\xE9partition du produit dans l\u2019organisme,
          elle a d\xFB le laisser circuler au moins 4h."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Dans un corps vivant j\u2019entends."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Or si elle avait attendu ne serait-ce que vingt minutes avant de se
          passer la corde au cou, elle se serait endormie lourdement.
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Effectivement c\u2019est \xE9trange."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Et concernant le reste du corps, vous avez remarqu\xE9 quelque chose
          d\u2019anormal ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Eh bien, il y a les marques de la corde bien s\xFBr, mais \xE0 part
          \xE7a aucune trace de violence, ni coups ni m\xEAme bousculade"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 100000002c000000
        eventTrigger: 
      canAskQuestions: 0
      endDialog: 1
    - dialog:
      - quote: Clara Grey est une femme charmante, j'aimerais l'aider.
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 1
    - dialog:
      - quote: "J\u2019aimerais en savoir plus sur vous."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Docteur Lou Ellis, affect\xE9e \xE0 cet h\xF4pital depuis 2 ans."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Sortie major de promo de l\u2019Universit\xE9 de m\xE9decine de New
          York."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Sp\xE9cialis\xE9e en h\xE9matologie et m\xE9decine l\xE9gale."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Sacr\xE9 palmar\xE8s."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est \xE0 peine suffisant pour faire du caf\xE9 ici."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vos coll\xE8gues ne vous appr\xE9cient pas \xE0 votre juste valeur
          ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je suis la premi\xE8re femme m\xE9decin de New York, je ne m\u2019attendais
          pas \xE0 une sin\xE9cure quand j\u2019ai rejoint le service."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Mais je ne supporte plus de voir mon travail d\xE9pr\xE9ci\xE9 par
          ces abrutis."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je sais ce que c\u2019est de se d\xE9mener pour moins que rien de
          remerciement."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "On se dit \xAB \xE0 quoi bon faire tout \xE7a ? Tout le monde s\u2019en
          fout. \xBB. Et le pire, c\u2019est que c\u2019est vrai."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Tout le monde s\u2019en fout."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Mais si on ne fait pas ce qu\u2019on a \xE0 faire au moment o\xF9
          on doit le faire, personne ne le fera \xE0 notre place."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous ne serez s\xFBrement jamais reconnue pour votre travail."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Des hommes moins comp\xE9tent s\u2019attribueront les m\xE9rites de
          vos efforts."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Mais \xE7a ne doit pas avoir d\u2019importance."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je ne m\u2019attendais pas \xE0 de la philosophie de la part d\u2019un
          priv\xE9."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "La guerre \xE7a rend philosophe."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Tiens donc ?
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Laissez, je divague.
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "Tenez, j\u2019ai trouv\xE9 \xE7a dans l\u2019appartement de Loraine.
          On dirait de la drogue."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Faites voir\u2026"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\xC7a pourrait correspondre avec ce qu\u2019on a trouv\xE9 dans le
          sang de la victime."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je vais comparer avec les r\xE9sultats du rapport."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\u2026"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oui c\u2019est bien \xE7a !"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est cette drogue que Loraine a ing\xE9r\xE9 le soir de sa mort."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 2f000000
        eventTrigger: 
      - quote: "Je m\u2019en doutais."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous pensez qu\u2019elle en prenait fr\xE9quemment ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Non, au contraire. Ce devait \xEAtre la premi\xE8re fois qu\u2019elle
          en prenait d\u2019apr\xE8s les analyses sanguines."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Int\xE9ressant."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "Vous pouvez m\u2019en dire plus sur ces marques de corde ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "D\u2019apr\xE8s l'\xE9tat des h\xE9matomes sur le cou, la victime
          \xE9tait vivante lorsqu\u2019elle a \xE9t\xE9 pendue."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est donc bel et bien la corde qui est responsable de sa mort."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Reste \xE0 d\xE9couvrir comment elle est arriv\xE9e l\xE0."
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "Vous avez autre chose \xE0 me montrer ?"
        characterName: Dr. Lou Ellis
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
