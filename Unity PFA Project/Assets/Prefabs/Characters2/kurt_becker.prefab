  allDialogs:
    listOfDialogs:
    - dialog:
      - quote: "Pardon? Assassin\xE9e vous dites?"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est ce que pense ma cliente. "
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Bon. J\u2019accepte de vous aider, mais pas plus de trente minutes."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Je pense que ce sera amplement suffisant.
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\xC9coutez, j\u2019aimais Loraine plus que tout au monde."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je ne comprends pas ce qu\u2019il s\u2019est pass\xE9!"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Du calme, du calme. Reprenons depuis le d\xE9but, voulez-vous ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous \xEAtes bien Kurt Becker, le petit ami de Loraine Grey ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est moi oui."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 04000000
        eventTrigger: 
      - quote: "Bien. Pourriez-vous me d\xE9crire la soir\xE9e avant la mort de Loraine."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je vais vous dire ce que j\u2019ai dit \xE0 la police."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019\xE9tait un dimanche soir ordinaire."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Loraine travaillait dans la chambre, et moi je regardais une \xE9mission
          de t\xE9l\xE9."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vers 22h, on est all\xE9s se coucher. Je n\u2019ai vraiment rien remarqu\xE9
          d\u2019anormal."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Et le lendemain matin, j\u2019ai vu le corps de ma Loraine pendu \xE0
          la poutre du salon."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Il m\u2019a fallu un peu de temps pour reprendre mes esprits,"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "mais d\xE8s que j\u2019ai pu j\u2019ai appel\xE9 les flics qui ont
          d\xE9barqu\xE9s dans le quart d\u2019heure."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Ils m\u2019ont pos\xE9 un tas de question, ils ont embarqu\xE9 le
          corps et puis voil\xE0."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Apr\xE8s \xE7a j\u2019ai picol\xE9 un peu, je pense que c\u2019\xE9tait
          mon droit."
        characterName: Kurt Becker
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Ah oui, un journaliste est venu m'interroger aussi dans l\u2019apr\xE8s-midi."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Comment ils font pour \xEAtre au courant si vite ces connards ?"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Loraine avait-elle un comportement suspect ? Ou inhabituel ?
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Bah\u2026 C\u2019est vrai qu\u2019elle a allum\xE9 un feu dans la
          chemin\xE9e alors qu\u2019il ne faisait pas froid,"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "mais \xE0 part \xE7a, non, rien\u2026"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Est-ce que vous avez des ennemis ? Quelqu\u2019un qui pourrait vous
          en vouloir ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Non, pas du tout !
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "On \xE9tait toujours entre nous, ce n\u2019\xE9tait pas notre genre
          de se mettre dans des affaires louches."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Bon. Vous me permettez de faire le tour de votre appartement ?
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je suis s\xFBr que j\u2019y trouverais des indices."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Euh\u2026 Oui allez-y. Mais ne fouillez pas trop quand m\xEAme\u2026"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je vous remercie. Je n\u2019en ai que pour un instant."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 0
      endDialog: 1
    - dialog:
      - quote: "Vous \xEAtes encore l\xE0 ?"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: Vous pouvez me parler de Loraine ?
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Loraine\u2026 C\u2019est la seule chose bien qui me soit arriv\xE9e
          dans la vie."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je ne sais pas ce qui a pouss\xE9 une perle comme elle \xE0 poser
          les yeux sur un d\xE9chet comme moi."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Mais elle l\u2019a fait. On \xE9tait ensemble depuis deux mois\u2026
          \xC7a semble peu pour s\u2019attacher hein ?"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "L\u2019amour, \xE7a ne se discute pas."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Mouais. Bah pour \xEAtre accro, \xE7a j\u2019\xE9tais accro. \xC7a
          a \xE9t\xE9 les deux mois les plus heureux de ma vie."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Et Loraine, comment \xE9tait-elle ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Un rayon de soleil. Elle avait un sourire magnifique.
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "On passait beaucoup de temps ensemble. Tout notre temps en fait, vu
          qu\u2019on ne travaillait pas."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Aucun de vous ne travaillait ? Mais comment vous faisiez pour l\u2019argent
          ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "On se d\xE9brouillait. On n\u2019\xE9tait pas riche mais on n\u2019en
          avait pas besoin."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Bordel\u2026 Ce qu\u2019elle me manque."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 34000000
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "Qu\u2019est ce que vous pouvez me dire sur vous ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Sur moi ?
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Bah\u2026 Je suis un ch\xF4meur, r\xE9cemment veuf. Il n\u2019y a
          rien \xE0 dire de plus."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous savez, c\u2019est pas si grave d\u2019\xEAtre un looser."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Vous avez de la famille ? Des amis ?
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "J\u2019ai un fr\xE8re, c\u2019est tout\u2026"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\xC7a va ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Ce n\u2019est rien. On peut changer de sujet ?"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Comme vous voudrez.
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "J\u2019ai trouv\xE9 \xE7a dans votre chambre, c\u2019est \xE0 vous
          ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oh merde\u2026 Non ce n\u2019est pas ce que vous croyez."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Vous en faites pas, je ne suis pas de la police.
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je veux juste savoir si c\u2019est \xE0 vous."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Non. C\u2019\xE9tait\u2026 \xE0 Loraine. Elle en prenait de temps
          en temps."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous avez oubli\xE9 de m\u2019en parler."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oui ce n\u2019est pas vraiment le genre de choses dont j\u2019aime
          parler."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "J\u2019ai trouv\xE9 une valise dans votre chambre. Elle \xE9tait remplie
          de v\xEAtements."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: Ah ?
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous avez l\u2019intention de partir ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oui, c\u2019est \xE7a. Je me suis dit que changer d\u2019air me ferais
          sans doute du bien."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je suis d\u2019accord, et vous partez o\xF9 comme \xE7a ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je\u2026 n\u2019ai pas encore d\xE9cid\xE9."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "J\u2019ai trouv\xE9 \xE7a dans la chemin\xE9e, mais je n\u2019y comprends
          rien."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Ah \xE7a ? \xC7a doit \xEAtre un po\xE8me rat\xE9 de Loraine."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Un po\xE8me ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oui, c\u2019est sur \xE7a qu\u2019elle travaillait l\u2019autre soir."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Elle aimait \xE9crire des po\xE8mes de temps en temps\u2026"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je n\u2019y ai jamais rien compris, ils sont tous \xE9crits dans une
          esp\xE8ce de langage bizarre."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vous les avez d\xE9j\xE0 lus ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "J\u2019vous dit, je n\u2019y comprenais rien."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Les trucs de gonzesses \xE7a ne me r\xE9ussit pas trop alors je la
          laissais faire."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Parfois elle \xE9tait tellement \xE9nerv\xE9e de ne pas r\xE9ussir
          \xE0 \xE9crire qu\u2019elle jetait tout au feu."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est ce qui a d\xFB arriver \xE0 celui-l\xE0 d\u2019ailleurs."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est \xE9trange\u2026 Elle les faisait lire \xE0 quelqu\u2019un
          ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Oui je crois, elle avait un ami \xE0 qui elle les envoyait."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Mais je ne l\u2019ai jamais vu."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je ne vois pas trop qui \xE7a peut int\xE9resser ces conneries, mais
          bon, \xE7a lui faisait plaisir alors\u2026"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 32000000
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: Vous lisez ce genre de magazine vous ?
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\xC7a m\u2019arrive oui, mais cette fois je l\u2019ai achet\xE9 parce
          qu\u2019il parlait de Loraine."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Vu que sa s\u0153ur est actrice, \xE7a a attir\xE9 les journalistes,
          mais je veux plus avoir affaire \xE0 eux."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: Rien que des ordures qui profitent du malheur des autres.
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "\xC7a vous d\xE9range si je l\u2019emporte avec moi ?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Je l\u2019aurais foutu \xE0 la poubelle de toute fa\xE7on. Allez-y."
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "J\u2019ai d\xE9couvert comment \xE9tait morte votre femme."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: 'Comment? '
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "On l\u2019a drogu\xE9e. Elle s\u2019est endormie et l\u2019assassin
          en a profit\xE9 pour lui passer la corde au cou."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "C\u2019est impossible\u2026 Non vous devez vous tromper !"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Il me reste encore quelques indices \xE0 trouver mais je sens que
          je me rapproche de la v\xE9rit\xE9."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      canAskQuestions: 1
      endDialog: 0
    - dialog:
      - quote: "Dites-moi Kurt, cette drogue, elle \xE9tait \xE0 vous pas vrai?"
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Pardon? Pas du tout! C\u2019\xE9tait \xE0 Loraine je vous ai dit!"
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "D\u2019apr\xE8s les preuves que j\u2019ai ici, vous mentez."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: 
      - quote: '...'
        characterName: Kurt Becker
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: 
      - quote: "Bon oui c\u2019est \xE0 moi et alors? C\u2019est ma vie priv\xE9e
