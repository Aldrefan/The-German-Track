      - quote: Marvin Meyer ?
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: []
        spriteCharacter: {fileID: 0}
      - quote: "Qui est \xE0 l\u2019appareil ?"
        characterName: Marvin Meyer
        isInLeftSide: 0
        newStickerIndexList: 
        eventTrigger: []
        spriteCharacter: {fileID: 0}
      - quote: "Je m\u2019appelle Kenneth, je suis un ami de Scott Nelson."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: []
        spriteCharacter: {fileID: 0}
      - quote: "J\u2019ai des questions \xE0 vous poser."
        characterName: Kenneth
        isInLeftSide: 1
        newStickerIndexList: 
        eventTrigger: []
        spriteCharacter: {fileID: 0}
      - quote: "Je vous \xE9coute. Mais si la conversation devient trop indiscr\xE8te,
